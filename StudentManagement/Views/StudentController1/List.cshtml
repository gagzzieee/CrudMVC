@{
}
@model List<StudentManagement.Models.Entities.Student>
<h1>Student List</h1>


<table class=" table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Roll Number</th>
            <th>CGPA</td>
        </tr>
    </thead>

    <tbody>
        @foreach(var student in Model)
        {
            <tr>
            <td>@student.Id</td>
            
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>@student.RollNumber</td>
                <td>@student.CGPA</td>

            </tr>
            
            
        }

    </tbody>
</table>